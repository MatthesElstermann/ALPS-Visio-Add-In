<Window x:Class="VisioAddIn.WindowSnapMaintenance"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        SizeToContent="WidthAndHeight" WindowStartupLocation="CenterScreen" MinWidth="200">
    <Grid Margin="15">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>

        <Label Name="labelShapeSnapName" ></Label>
        <Label Name="labelShapeSnapToName" Grid.Row="1" Margin="0,8,0,0"></Label>

        <WrapPanel Grid.Row="2" HorizontalAlignment="Right" Margin="0,15,0,0">
            <Button IsDefault="True" Name="btnDialogYes" Click="btnDialogYes_Click" MinWidth="60" Margin="0,0,10,0"></Button>
            <Button IsCancel="True" Name="btnDialogNo" Click="btnDialogNo_Click" MinWidth="60"></Button>
        </WrapPanel>
    </Grid>
</Window>

